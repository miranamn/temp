<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FFEFD5;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.FXMLMainForm">
    <children>
        <TableView fx:id="table" layoutX="85.0" layoutY="74.0" prefHeight="204.0" prefWidth="430.0" style="-fx-background-color: #D8BFD8; -fx-background-radius: 30;">
            <columns>
                <TableColumn fx:id="columnX" prefWidth="214.0" text="X value" />
                <TableColumn fx:id="columnY" prefWidth="219.0" text="Y value" />
            </columns>
        </TableView>
        <TextField fx:id="edX" layoutX="184.0" layoutY="302.0" prefHeight="26.0" prefWidth="110.0" style="-fx-background-color: #D8BFD8; -fx-background-radius: 20;" />
        <TextField fx:id="edY" layoutX="184.0" layoutY="342.0" prefHeight="26.0" prefWidth="110.0" style="-fx-background-color: #D8BFD8; -fx-background-radius: 20;" />
        <Label layoutX="102.0" layoutY="60.0" prefHeight="18.0" prefWidth="0.0" />
        <Button fx:id="Addpoint" layoutX="312.0" layoutY="302.0" mnemonicParsing="false" onAction="#Addpoint" prefHeight="26.0" prefWidth="82.0" style="-fx-background-radius: 20;" text="Add point " textFill="DARKRED" />
        <Button fx:id="Delete" layoutX="312.0" layoutY="342.0" mnemonicParsing="false" onAction="#Delete" prefHeight="26.0" prefWidth="82.0" style="-fx-background-radius: 20;" text="Delete" textFill="DARKRED" />
        <Label layoutX="93.0" layoutY="306.0" prefHeight="18.0" prefWidth="75.0" style="-fx-background-color: #FFDAB9;" text="New point X:" textFill="DARKRED" />
        <Label layoutX="93.0" layoutY="346.0" prefHeight="18.0" prefWidth="75.0" style="-fx-background-color: #FFDAB9;" text="New point Y:" textFill="DARKRED" />
        <Label fx:id="label" layoutX="131.0" layoutY="50.0" prefHeight="18.0" prefWidth="68.0" text="Point 2 of 7" />
        <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="600.0" style="-fx-background-color: #D8BFD8; -fx-background-radius: 15;">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#NewFile" text="New file" />
                        <MenuItem mnemonicParsing="false" onAction="#OpenFile" text="Open file" />
                        <MenuItem mnemonicParsing="false" onAction="#SaveFile" text="Save file" />
                        <MenuItem mnemonicParsing="false" onAction="#SaveAsFile" text="Save as file" />
                        <MenuItem fx:id="Exit" mnemonicParsing="false" text="Exit" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Tabulate">
                    <items>
                        <MenuItem fx:id="Download" mnemonicParsing="false" text="Download" />
                        <MenuItem fx:id="Tabulate" mnemonicParsing="false" text="Tabulate" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>
</AnchorPane>
